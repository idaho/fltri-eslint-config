{
  "name": "@fltri/eslint-config",
  "private": false,
  "version": "1.1.4",
  "description": "My custom ESLint config set.",
  "main": "node.js",
  "author": "idaho",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idaho/fltri-eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/idaho/fltri-eslint-config/issues"
  },
  "homepage": "https://github.com/idaho/fltri-eslint-config#readme",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "rimraf lib/ node.js web.js && npx tsc -b tsconfig.build.json",
    "prelint": "npm run compile",
    "lint": "npx eslint ./ --ext .ts,.json,.js",
    "qa": "npm run lint"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "eslint-plugin-better-styled-components": "1.1.2",
    "eslint-plugin-extended": "0.2.0",
    "eslint-plugin-jest": "27.0.4",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-mocha": "10.1.0",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-unicorn": "43.0.2",
    "glob-parent": "6.0.2",
    "hosted-git-info": "5.1.0",
    "normalize-url": "7.1.0",
    "path-parse": "1.0.7",
    "postcss": "8.4.16",
    "trim-newlines": "4.0.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.6",
    "@semantic-release/npm": "9.0.1",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/eslint": "8.4.6",
    "@types/jest": "29.0.2",
    "@types/node": "18.7.17",
    "ansi-regex": "6.0.1",
    "eslint": "8.23.1",
    "jest": "29.0.3",
    "react": "18.2.0",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.5",
    "ts-jest": "29.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.8.3"
  }
}
