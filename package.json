{
  "name": "@fltri/eslint-config",
  "private": false,
  "version": "1.0.53",
  "description": "My custom ESLint config set.",
  "main": "node.js",
  "author": "idaho",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idaho/fltri-eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/idaho/fltri-eslint-config/issues"
  },
  "homepage": "https://github.com/idaho/fltri-eslint-config#readme",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "rimraf lib/ node.js web.js && npx tsc -b tsconfig.build.json",
    "prelint": "npm run compile",
    "lint": "npx eslint ./ --ext .ts,.json,.js",
    "qa": "npm run lint"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.0",
    "eslint-plugin-better-styled-components": "1.1.2",
    "eslint-plugin-extended": "0.2.0",
    "eslint-plugin-jest": "25.3.2",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-mocha": "10.0.3",
    "eslint-plugin-radar": "0.2.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-unicorn": "39.0.0",
    "glob-parent": "6.0.2",
    "hosted-git-info": "4.0.2",
    "normalize-url": "7.0.2",
    "path-parse": "1.0.7",
    "postcss": "8.4.5",
    "trim-newlines": "4.0.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.2",
    "@semantic-release/npm": "8.0.3",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/eslint": "8.2.1",
    "@types/jest": "27.0.3",
    "@types/node": "17.0.5",
    "ansi-regex": "6.0.1",
    "eslint": "8.5.0",
    "jest": "27.4.5",
    "react": "17.0.2",
    "rimraf": "3.0.2",
    "semantic-release": "18.0.1",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  }
}
